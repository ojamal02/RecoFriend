<div class="container">

    <h1 class="titlecard"><span>RecoFriend List Index</span></h1>

</div>

<br></br>

<div class="row">

  <div class="large-8 large-centered columns">
      <div id="app">

      <div class="hidden">
        <ul>
          <% @lists.each do |list| %>
            <li><%= link_to list.title, list_path(list) %> "-" <%= list.user.name %></li>
          <% end %>
        </ul>
      </div>
      </div>
    </div>

  </div>
</div>
